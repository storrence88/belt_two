<br>
<a href="/songs">Dashboard</a>
<br>
<h3>Others who added <%= @this_song.artist %> - <%= @this_song.title %></h3>
<br>
<table class="table table-striped">
    <thead class="thead-light">
        <tr>
            <th>Name</th>
            <th>Times Added</th>
        </tr>
    </thead>
    <tbody>
        <% if @users_added %>
            <% @users_added.each do |x| %>
                <tr>
                    <td><a href="/users/<%= User.find(x.user_id).id %>"><%= User.find(x.user_id).first_name %> <%= User.find(x.user_id).last_name %></a></td>
                    <td><%= Playlist.all.where(song_id: @this_song.id).where(user_id: User.find(x.user_id).id).count %></td>
                </tr>
            <% end %>
        <% end %>
    </tbody>
</table>